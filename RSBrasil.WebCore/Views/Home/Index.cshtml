@{
    Layout = null;

}

<div class="container">
    <div class="login-wrap">
        <h2>RSBrasil</h2>

        <div class="form">
            <input type="text" id="txtUsuario" placeholder="Usuário" />

            <input type="password" id="txtSenha" placeholder="Senha" />
            <button class="btn" onclick="Logar()"> Logar </button>
        </div>
    </div>
</div>
@section css
    {
    <link rel="stylesheet" href="~/Content/login.css" />
    <link rel="stylesheet" href="~/Content/sweetalert2/sweetalert2.min.css" />
}
@section Scripts
    {
    <script src="@Url.Content("~/Scripts/pages/login.js")"></script>
    <script src="@Url.Content("~/Scripts/sweetalert2/sweetalert2.min.js")"></script>
}
